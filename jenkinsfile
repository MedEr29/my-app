pipeline {
    agent any
    stages {
        stage('Code Analysis') {
            steps {
                withSonarQubeEnv('SonarQube') {
                    bat "sonar-scanner.bat -Dsonar.projectKey=my-app -Dsonar.sources=src -Dsonar.host.url=http://localhost:9000"
                }
            }
        }
    }
}
